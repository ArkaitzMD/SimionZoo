<RLSimion FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>1</Num-Integration-Steps>
		<Delta-T>0.1</Delta-T>
		<Dynamic-Model>
			<Model>
		<Robot-control>
		</Robot-control>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>1500</Num-Episodes>
		<Eval-Freq>20</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>200</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.9</Gamma>
		<Freeze-Target-Function>true</Freeze-Target-Function>
		<Use-Importance-Weights>false</Use-Importance-Weights>
		<Experience-Replay>
			<Buffer-Size>1000</Buffer-Size>
			<Update-Batch-Size>10</Update-Batch-Size>
		</Experience-Replay>
		<State-Feature-Map>
			<Type>
		<RBF-State-Grid>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>robot1-x</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>robot1-y</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>robot1-theta</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
		</RBF-State-Grid>
		</Type>
		</State-Feature-Map>
		<Simion>
			<Type>
		<Inc-Natural-Actor-Critic>
			<VFunction>
				<Init-Value>0.0</Init-Value>
			</VFunction>
			<V-ETraces>
				<Threshold>0.001</Threshold>
				<Lambda>0.9</Lambda>
				<Replace>true</Replace>
			</V-ETraces>
			<U-ETraces>
				<Threshold>0.001</Threshold>
				<Lambda>0.9</Lambda>
				<Replace>true</Replace>
			</U-ETraces>
			<Alpha-v>
				<Schedule>
		<Constant>
			<Value>0.1</Value>
		</Constant>
		</Schedule>
			</Alpha-v>
			<Alpha-r>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha-r>
			<Alpha-u>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha-u>
			<Policy>
				<Policy>
		<Stochastic-Policy-Gaussian-Noise>
			<Output-Action>robot1-omega</Output-Action>
			<Mean-VFA>
				<Init-Value>0.0</Init-Value>
			</Mean-VFA>
			<Sigma-VFA>
				<Init-Value>0.0</Init-Value>
			</Sigma-VFA>
		</Stochastic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
			<Policy>
				<Policy>
		<Stochastic-Policy-Gaussian-Noise>
			<Output-Action>robot1-v</Output-Action>
			<Mean-VFA>
				<Init-Value>0.0</Init-Value>
			</Mean-VFA>
			<Sigma-VFA>
				<Init-Value>0.0</Init-Value>
			</Sigma-VFA>
		</Stochastic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</Inc-Natural-Actor-Critic>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion>
