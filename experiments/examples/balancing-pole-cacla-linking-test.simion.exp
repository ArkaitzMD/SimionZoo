<RLSimion FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>True</Log-eval-episodes>
		<Log-training-episodes>False</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>4</Num-Integration-Steps>
		<LINKED-NODE Name="Delta-T" LinkOriginNode="Log-Freq"/>
		<Dynamic-Model>
			<Model>
		<Balancing-pole>
		</Balancing-pole>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>10000</Num-Episodes>
		<Eval-Freq>100</Eval-Freq>
		<Progress-Update-Freq>1.17</Progress-Update-Freq>
		<Episode-Length>100</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.9</Gamma>
		<Freeze-Target-Function>true</Freeze-Target-Function>
		<Use-Importance-Weights>false</Use-Importance-Weights>
		<Experience-Replay>
			<Buffer-Size>1000</Buffer-Size>
			<Update-Batch-Size>10</Update-Batch-Size>
		</Experience-Replay>
		<State-Feature-Map>
			<Type>
		<RBF-State-Grid>
			<RBF-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>theta_dot</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>x</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>x_dot</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>theta</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
		</RBF-State-Grid>
		</Type>
		</State-Feature-Map>
		<Simion>
			<Type>
		<Actor-Critic>
			<Actor>
				<Output>
					<Policy-Learner>
		<CACLA>
			<Alpha>
				<Schedule>
		<Simple-Linear-Decay>
			<FORKED-NODE Name="Initial-Value" Alias="Actor-Gain">
			  <FORK-VALUE Name="Value-0">
			  	<Initial-Value>0.1</Initial-Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-1">
			  	<Initial-Value>0.075</Initial-Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-2">
			  	<Initial-Value>0.05</Initial-Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-3">
			  	<Initial-Value>0.025</Initial-Value>
			  </FORK-VALUE>
			</FORKED-NODE>
			<End-Value>0.0</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Alpha>
			<Policy>
				<Policy>
		<Deterministic-Policy-Gaussian-Noise>
			<Output-Action>force</Output-Action>
			<Deterministic-Policy-VFA>
				<Init-Value>0.0</Init-Value>
			</Deterministic-Policy-VFA>
			<Exploration-Noise>
				<Noise>
		<GaussianNoise>
			<Sigma>1</Sigma>
			<Alpha>1.0</Alpha>
			<Scale>
				<Schedule>
		<Simple-Linear-Decay>
			<FORKED-NODE Name="Initial-Value" Alias="Noise-Width">
			  <FORK-VALUE Name="Value-0">
			  	<Initial-Value>1.17</Initial-Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-1">
			  	<Initial-Value>0.5</Initial-Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-2">
			  	<Initial-Value>0.1</Initial-Value>
			  </FORK-VALUE>
			</FORKED-NODE>
			<End-Value>0.0</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Scale>
		</GaussianNoise>
		</Noise>
			</Exploration-Noise>
		</Deterministic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</CACLA>
		</Policy-Learner>
				</Output>
			</Actor>
			<Critic>
				<Critic>
		<TD-Lambda>
			<Alpha>
				<Schedule>
		<Constant>
			<FORKED-NODE Name="Value" Alias="Critic-Gain">
			  <FORK-VALUE Name="Value-0">
			  	<Value>0.1</Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-1">
			  	<Value>0.075</Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-2">
			  	<Value>0.05</Value>
			  </FORK-VALUE>
			  <FORK-VALUE Name="Value-3">
			  	<Value>0.025</Value>
			  </FORK-VALUE>
			</FORKED-NODE>
		</Constant>
		</Schedule>
			</Alpha>
			<V-Function>
				<Init-Value>0.0</Init-Value>
			</V-Function>
		</TD-Lambda>
		</Critic>
			</Critic>
		</Actor-Critic>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion>
