<RLSimion-x64 FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>1</Num-Integration-Steps>
		<Delta-T>1</Delta-T>
		<Dynamic-Model>
			<Model>
		<Discrete-Gridworld>
			<Target-X>-4</Target-X>
			<Target-Y>5</Target-Y>
		</Discrete-Gridworld>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>1000</Num-Episodes>
		<Eval-Freq>10</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>500.0</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.9</Gamma>
		<Freeze-Target-Function>false</Freeze-Target-Function>
		<Use-Importance-Weights>false</Use-Importance-Weights>
		<State-Feature-Map>
			<Type>
		<Discrete-State-Grid>
			<Grid-Dimension>
				<Step-Size>1.0</Step-Size>
				<Variable>x-position</Variable>
			</Grid-Dimension>
			<Grid-Dimension>
				<Step-Size>1.0</Step-Size>
				<Variable>y-position</Variable>
			</Grid-Dimension>
		</Discrete-State-Grid>
		</Type>
		</State-Feature-Map>
		<Action-Feature-Map>
			<Type>
		<Discrete-Action-Grid>
			<Grid-Dimension>
				<Step-Size>1.0</Step-Size>
				<Variable>step</Variable>
			</Grid-Dimension>
		</Discrete-Action-Grid>
		</Type>
		</Action-Feature-Map>
		<Simion>
			<Type>
		<DQN>
			<Policy>
				<Policy>
		<Discrete-Deep-Policy>
			<neural-network><Problem xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Inputs>
    <InputData ID="state-input" Name="State">
      <Shape xsi:type="IntTuple1D">
        <x1>2</x1>
      </Shape>
    </InputData>
  </Inputs>
  <OptimizerSetting>
    <Optimizer xsi:type="OptimizerAdam">
      <Parameters>
        <OptimizerParameterOfStringDouble>
          <Key>Learning rate</Key>
          <Value>0.001</Value>
        </OptimizerParameterOfStringDouble>
        <OptimizerParameterOfStringDouble>
          <Key>Momentum</Key>
          <Value>0.9</Value>
        </OptimizerParameterOfStringDouble>
        <OptimizerParameterOfStringDouble>
          <Key>Variance momentum</Key>
          <Value>0.999</Value>
        </OptimizerParameterOfStringDouble>
        <OptimizerParameterOfStringDouble>
          <Key>Epsilon</Key>
          <Value>1E-08</Value>
        </OptimizerParameterOfStringDouble>
      </Parameters>
    </Optimizer>
  </OptimizerSetting>
  <Output>
    <LinkConnection TargetID="C9F8C93C" />
  </Output>
  <NetworkArchitecture>
    <Chains>
      <Chain Name="Chain">
        <ChainLinks>
          <LinkBase xsi:type="InputLayer" Name="Input Layer" ID="81FAAAEA">
            <Parameters>
              <ParameterBase xsi:type="InputDataParameter" Name="Input Data">
                <Value>state-input</Value>
              </ParameterBase>
            </Parameters>
          </LinkBase>
          <LinkBase xsi:type="DenseLayer" Name="Dense Layer" ID="DCE43972">
            <Parameters>
              <ParameterBase xsi:type="IntParameter" Name="Units">
                <Value>25</Value>
              </ParameterBase>
              <ParameterBase xsi:type="ActivationFunctionParameter" Name="Activation">
                <Value>relu</Value>
              </ParameterBase>
            </Parameters>
          </LinkBase>
          <LinkBase xsi:type="DenseLayer" Name="Dense Layer" ID="C9F8C93C">
            <Parameters>
              <ParameterBase xsi:type="IntParameter" Name="Units">
                <Value>4</Value>
              </ParameterBase>
              <ParameterBase xsi:type="ActivationFunctionParameter" Name="Activation">
                <Value>linear</Value>
              </ParameterBase>
            </Parameters>
          </LinkBase>
        </ChainLinks>
      </Chain>
    </Chains>
  </NetworkArchitecture>
</Problem></neural-network>
			<Output-Action>step</Output-Action>
			<epsilon>
				<Schedule>
		<Linear-Schedule>
			<Start-Offset>0.0</Start-Offset>
			<End-Offset>0.6</End-Offset>
			<Pre-Offset-Value>1.0</Pre-Offset-Value>
			<Initial-Value>1.0</Initial-Value>
			<End-Value>0.01</End-Value>
			<Evaluation-Value>0.0</Evaluation-Value>
			<Interpolation>quadratic</Interpolation>
			<Time-reference>experiment</Time-reference>
		</Linear-Schedule>
		</Schedule>
			</epsilon>
		</Discrete-Deep-Policy>
		</Policy>
			</Policy>
		</DQN>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion-x64>
