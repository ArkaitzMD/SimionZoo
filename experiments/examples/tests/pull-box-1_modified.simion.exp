<RLSimion-x64 FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>1</Num-Integration-Steps>
		<Delta-T>0.1</Delta-T>
		<Dynamic-Model>
			<Model>
		<Pull-Box-1>
		</Pull-Box-1>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>2000</Num-Episodes>
		<Eval-Freq>50</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>250</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.9</Gamma>
		<Freeze-Target-Function>true</Freeze-Target-Function>
		<Use-Importance-Weights>false</Use-Importance-Weights>
		<State-Feature-Map>
			<Type>
		<Tile-Coding-State-Grid>
			<Tile-layers>10</Tile-layers>
			<Tile-offset>0.1</Tile-offset>
			<Tile-Encoding-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>box-to-target-x</Variable>
				<Distribution>linear</Distribution>
			</Tile-Encoding-Grid-Dimension>
			<Tile-Encoding-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>box-to-target-y</Variable>
				<Distribution>linear</Distribution>
			</Tile-Encoding-Grid-Dimension>
			<Tile-Encoding-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>robot1-to-box-x</Variable>
				<Distribution>linear</Distribution>
			</Tile-Encoding-Grid-Dimension>
			<Tile-Encoding-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>robot1-to-box-y</Variable>
				<Distribution>linear</Distribution>
			</Tile-Encoding-Grid-Dimension>
			<Tile-Encoding-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>robot1-theta</Variable>
				<Distribution>linear</Distribution>
			</Tile-Encoding-Grid-Dimension>
		</Tile-Coding-State-Grid>
		</Type>
		</State-Feature-Map>
		<Simion>
			<Type>
		<Actor-Critic>
			<Actor>
				<Output>
					<Policy-Learner>
		<CACLA>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.1</Value>
		</Constant>
		</Schedule>
			</Alpha>
			<Policy>
				<Policy>
		<Deterministic-Policy-Gaussian-Noise>
			<Output-Action>robot1-omega</Output-Action>
			<Deterministic-Policy-VFA>
				<Init-Value>0.0</Init-Value>
			</Deterministic-Policy-VFA>
			<Exploration-Noise>
				<Noise>
		<Ornstein-Uhlenbeck>
			<Mu>0.0</Mu>
			<Sigma>1.0</Sigma>
			<Theta>0.5</Theta>
			<Scale>
				<Schedule>
		<Simple-Linear-Decay>
			<Initial-Value>1.0</Initial-Value>
			<End-Value>0.1</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Scale>
		</Ornstein-Uhlenbeck>
		</Noise>
			</Exploration-Noise>
		</Deterministic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</CACLA>
		</Policy-Learner>
				</Output>
				<Output>
					<Policy-Learner>
		<CACLA>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.1</Value>
		</Constant>
		</Schedule>
			</Alpha>
			<Policy>
				<Policy>
		<Deterministic-Policy-Gaussian-Noise>
			<Output-Action>robot1-v</Output-Action>
			<Deterministic-Policy-VFA>
				<Init-Value>0.0</Init-Value>
			</Deterministic-Policy-VFA>
			<Exploration-Noise>
				<Noise>
		<Ornstein-Uhlenbeck>
			<Mu>0.0</Mu>
			<Sigma>0.5</Sigma>
			<Theta>0.1</Theta>
			<Scale>
				<Schedule>
		<Simple-Linear-Decay>
			<Initial-Value>1.0</Initial-Value>
			<End-Value>0.5</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Scale>
		</Ornstein-Uhlenbeck>
		</Noise>
			</Exploration-Noise>
		</Deterministic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</CACLA>
		</Policy-Learner>
				</Output>
			</Actor>
			<Critic>
				<Critic>
		<TD-Lambda>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.1</Value>
		</Constant>
		</Schedule>
			</Alpha>
			<V-Function>
				<Init-Value>0.0</Init-Value>
			</V-Function>
		</TD-Lambda>
		</Critic>
			</Critic>
		</Actor-Critic>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion-x64>
