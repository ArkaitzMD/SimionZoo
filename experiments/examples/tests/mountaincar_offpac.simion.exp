<RLSimion FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>4</Num-Integration-Steps>
		<Delta-T>0.25</Delta-T>
		<Dynamic-Model>
			<Model>
		<Mountain-car>
		</Mountain-car>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>100</Num-Episodes>
		<Eval-Freq>1</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>5000</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.99</Gamma>
		<Freeze-Target-Function>false</Freeze-Target-Function>
		<Use-Importance-Weights>true</Use-Importance-Weights>
		<State-Feature-Map>
			<Type>
		<Tile-Coding-State-Grid>
			<Tile-layers>10</Tile-layers>
			<Tile-offset>0.05</Tile-offset>
			<Tile-Encoding-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>position</Variable>
				<Distribution>linear</Distribution>
			</Tile-Encoding-Grid-Dimension>
			<Tile-Encoding-Grid-Dimension>
				<Num-Features>10</Num-Features>
				<Variable>velocity</Variable>
				<Distribution>linear</Distribution>
			</Tile-Encoding-Grid-Dimension>
		</Tile-Coding-State-Grid>
		</Type>
		</State-Feature-Map>
		<Action-Feature-Map>
			<Type>
		<Discrete-Action-Grid>
			<Grid-Dimension>
				<Step-Size>1.0</Step-Size>
				<Variable>pedal</Variable>
			</Grid-Dimension>
		</Discrete-Action-Grid>
		</Type>
		</Action-Feature-Map>
		<Simion>
			<Type>
		<Off-Policy-Actor-Critic>
			<VFunction>
				<Init-Value>0.0</Init-Value>
			</VFunction>
			<ETraces>
				<Threshold>0.001</Threshold>
				<Lambda>0.9</Lambda>
				<Replace>true</Replace>
			</ETraces>
			<Alpha-v>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha-v>
			<Alpha-w>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha-w>
			<Alpha-u>
				<Schedule>
		<Constant>
			<Value>0.0</Value>
		</Constant>
		</Schedule>
			</Alpha-u>
			<b-Policy>
				<Policy>
		<Uniform-Policy>
			<Output-Action>pedal</Output-Action>
		</Uniform-Policy>
		</Policy>
			</b-Policy>
			<Policy>
				<Policy>
		<Stochastic-Policy-Gaussian-Noise>
			<Output-Action>pedal</Output-Action>
			<Mean-VFA>
				<Init-Value>0.0</Init-Value>
			</Mean-VFA>
			<Sigma-VFA>
				<Init-Value>0.0</Init-Value>
			</Sigma-VFA>
		</Stochastic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</Off-Policy-Actor-Critic>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion>
