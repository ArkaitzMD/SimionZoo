<RLSimion FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>4</Num-Integration-Steps>
		<Delta-T>0.25</Delta-T>
		<Dynamic-Model>
			<Model>
		<Mountain-car>
		</Mountain-car>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>100</Num-Episodes>
		<Eval-Freq>1</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>5000</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.99</Gamma>
		<Freeze-Target-Function>false</Freeze-Target-Function>
		<Use-Importance-Weights>true</Use-Importance-Weights>
		<State-Feature-Map>
		  <Num-Features-Per-Dimension>10</Num-Features-Per-Dimension>
		  <Input-State><Input-State>position</Input-State></Input-State>
		  <Input-State><Input-State>velocity</Input-State></Input-State>
		  <Feature-Mapper>
		    <Type>
		      <Tile-Coding>
			<Num-Tiles>5</Num-Tiles>
			<Tile-Offset>0.1</Tile-Offset>
		      </Tile-Coding>
		    </Type>
		  </Feature-Mapper>
		</State-Feature-Map>
		<Action-Feature-Map>
		  <Num-Features-Per-Dimension>20</Num-Features-Per-Dimension>
		  <Input-Action><Input-Action>pedal</Input-Action></Input-Action>
		  <Feature-Mapper>
		    <Type>
		      <Tile-Coding>
			<Num-Tiles>5</Num-Tiles>
			<Tile-Offset>0.05</Tile-Offset>
		      </Tile-Coding>
		    </Type>
		  </Feature-Mapper>
		</Action-Feature-Map>
		<Simion>
			<Type>
		<Off-Policy-Actor-Critic>
			<VFunction>
				<Init-Value>0.0</Init-Value>
			</VFunction>
			<ETraces>
				<Threshold>0.001</Threshold>
				<Lambda>0.9</Lambda>
				<Replace>true</Replace>
			</ETraces>
			<Alpha-v>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha-v>
			<Alpha-w>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha-w>
			<Alpha-u>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha-u>
                    <Policy>
                      <Policy>
                        <Deterministic-Policy-Gaussian-Noise>
                          <Output-Action>pedal</Output-Action>
                          <Deterministic-Policy-VFA>
                            <Init-Value>0.0</Init-Value>
                          </Deterministic-Policy-VFA>
                          <Exploration-Noise>
                            <Noise>
                              <Ornstein-Uhlenbeck>
                                <Mu>0.0</Mu>
                                <Sigma>1.0</Sigma>
                                <Theta>0.5</Theta>
                                <Scale>
                                  <Schedule>
                                    <Simple-Linear-Decay>
                                      <Initial-Value>1.0</Initial-Value>
                                      <End-Value>0.1</End-Value>
                                    </Simple-Linear-Decay>
                                  </Schedule>
                                </Scale>
                              </Ornstein-Uhlenbeck>
                            </Noise>
                          </Exploration-Noise>
                        </Deterministic-Policy-Gaussian-Noise>
                      </Policy>
                    </Policy>
		</Off-Policy-Actor-Critic>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion>
