<RLSimion FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>1</Num-Integration-Steps>
		<Delta-T>0.1</Delta-T>
		<Dynamic-Model>
			<Model>
		<Robot-control>
		</Robot-control>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>10000</Num-Episodes>
		<Eval-Freq>25</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>200</Episode-Length>
	</Experiment>
	<SimGod>
		<V-Function-Update-Freq>100</V-Function-Update-Freq>
		<Gamma>0.9</Gamma>
		<Count-State-Visits>false</Count-State-Visits>
		<Freeze-V-Function>true</Freeze-V-Function>
		<Use-Importance-Weights>false</Use-Importance-Weights>
		<Experience-Replay>
			<Buffer-Size>1000</Buffer-Size>
			<Update-Batch-Size>10</Update-Batch-Size>
		</Experience-Replay>
		<State-Feature-Map>
			<Type>
		<RBF-State-Grid>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>rx1</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>ry1</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>theta</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
		</RBF-State-Grid>
		</Type>
		</State-Feature-Map>
	<Action-Feature-Map>
			<Type>
		<RBF-Action-Grid>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>v</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
			<RBF-Grid-Dimension>
				<Num-Features>20</Num-Features>
				<Variable>omega</Variable>
				<Distribution>linear</Distribution>
			</RBF-Grid-Dimension>
		</RBF-Action-Grid>
		</Type>
		</Action-Feature-Map>
		<Simion>
			<Type>
		<Actor-Critic>
			<Actor>
				<Output>
					<Policy-Learner>
		<CACLA>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.0001</Value>
		</Constant>
		</Schedule>
			</Alpha>
			<Policy>
				<Policy>
		<Deterministic-Policy-Gaussian-Noise>
			<Output-Action>omega</Output-Action>
			<Deterministic-Policy-VFA>
				<Init-Value>0.0</Init-Value>
			</Deterministic-Policy-VFA>
			<Exploration-Noise>
				<Noise>
		<Ornstein-Uhlenbeck>
			<Mu>0.0</Mu>
			<Sigma>1.0</Sigma>
			<Theta>0.5</Theta>
			<Scale>
				<Schedule>
		<Simple-Linear-Decay>
			<Initial-Value>1.0</Initial-Value>
			<End-Value>0.0</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Scale>
		</Ornstein-Uhlenbeck>
		</Noise>
			</Exploration-Noise>
		</Deterministic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</CACLA>
		</Policy-Learner>
				</Output>
				<Output>
					<Policy-Learner>
		<CACLA>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.0001</Value>
		</Constant>
		</Schedule>
			</Alpha>
			<Policy>
				<Policy>
		<Deterministic-Policy-Gaussian-Noise>
			<Output-Action>v</Output-Action>
			<Deterministic-Policy-VFA>
				<Init-Value>0.0</Init-Value>
			</Deterministic-Policy-VFA>
			<Exploration-Noise>
				<Noise>
		<Ornstein-Uhlenbeck>
			<Mu>0.0</Mu>
			<Sigma>0.2</Sigma>
			<Theta>0.1</Theta>
			<Scale>
				<Schedule>
		<Simple-Linear-Decay>
			<Initial-Value>1.0</Initial-Value>
			<End-Value>0.0</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Scale>
		</Ornstein-Uhlenbeck>
		</Noise>
			</Exploration-Noise>
		</Deterministic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</CACLA>
		</Policy-Learner>
				</Output>
			</Actor>
			<Critic>
				<Critic>
		<Q-Learning>
			<Q-Function>
				<Init-Value>0.0</Init-Value>
			</Q-Function>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.01</Value>
		</Constant>
		</Schedule>
			</Alpha>
		</Q-Learning>
		</Critic>
			</Critic>
		</Actor-Critic>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion>
