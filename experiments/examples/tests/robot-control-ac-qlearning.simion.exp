<RLSimion FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
	</Log>
	<World>
		<Num-Integration-Steps>1</Num-Integration-Steps>
		<Delta-T>0.1</Delta-T>
		<Dynamic-Model>
			<Model>
		<Robot-control>
		</Robot-control>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>2000</Num-Episodes>
		<Eval-Freq>25</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>200</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.9</Gamma>
		<Count-State-Visits>false</Count-State-Visits>
		<Freeze-Target-Function>true</Freeze-Target-Function>
		<Use-Importance-Weights>false</Use-Importance-Weights>
		<Experience-Replay>
			<Buffer-Size>1000</Buffer-Size>
			<Update-Batch-Size>10</Update-Batch-Size>
		</Experience-Replay>
	<State-Feature-Map>
	  <Num-Features-Per-Dimension>20</Num-Features-Per-Dimension>
	  <Input-State><Input-State>robot1-x</Input-State></Input-State>
	  <Input-State><Input-State>robot1-y</Input-State></Input-State>
	  <Input-State><Input-State>robot1-theta</Input-State></Input-State>
	  <Feature-Mapper>
	    <Type>
	      <Gaussian-RBF-Grid/>
	    </Type>
	  </Feature-Mapper>
	</State-Feature-Map>
	<Action-Feature-Map>
	  <Num-Features-Per-Dimension>20</Num-Features-Per-Dimension>
	  <Input-Action><Input-Action>robot1-v</Input-Action></Input-Action>
	  <Input-Action><Input-Action>robot1-omega</Input-Action></Input-Action>
	  <Feature-Mapper>
	    <Type>
	      <Gaussian-RBF-Grid/>
	    </Type>
	  </Feature-Mapper>
	</Action-Feature-Map>
		<Simion>
			<Type>
		<Actor-Critic>
			<Actor>
				<Output>
					<Policy-Learner>
		<CACLA>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha>
			<Policy>
				<Policy>
		<Deterministic-Policy-Gaussian-Noise>
			<Output-Action>robot1-omega</Output-Action>
			<Deterministic-Policy-VFA>
				<Init-Value>0.0</Init-Value>
			</Deterministic-Policy-VFA>
			<Exploration-Noise>
				<Noise>
		<Ornstein-Uhlenbeck>
			<Mu>0.0</Mu>
			<Sigma>1.0</Sigma>
			<Theta>0.5</Theta>
			<Scale>
                                  <Schedule>
                                    <Constant>
                                      <Value>1.0</Value>
                                    </Constant>
                                  </Schedule>
                                </Scale>
		</Ornstein-Uhlenbeck>
		</Noise>
			</Exploration-Noise>
		</Deterministic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</CACLA>
		</Policy-Learner>
				</Output>
				<Output>
					<Policy-Learner>
		<CACLA>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.001</Value>
		</Constant>
		</Schedule>
			</Alpha>
			<Policy>
				<Policy>
		<Deterministic-Policy-Gaussian-Noise>
			<Output-Action>robot1-v</Output-Action>
			<Deterministic-Policy-VFA>
				<Init-Value>0.0</Init-Value>
			</Deterministic-Policy-VFA>
			<Exploration-Noise>
				<Noise>
		<Ornstein-Uhlenbeck>
			<Mu>0.0</Mu>
			<Sigma>0.2</Sigma>
			<Theta>0.1</Theta>
<Scale>
                                  <Schedule>
                                    <Constant>
                                      <Value>1.0</Value>
                                    </Constant>
                                  </Schedule>
                                </Scale>
		</Ornstein-Uhlenbeck>
		</Noise>
			</Exploration-Noise>
		</Deterministic-Policy-Gaussian-Noise>
		</Policy>
			</Policy>
		</CACLA>
		</Policy-Learner>
				</Output>
			</Actor>
			<Critic>
				<Critic>
		<Q-Learning>
			<Q-Function>
				<Init-Value>0.0</Init-Value>
			</Q-Function>
			<Alpha>
				<Schedule>
		<Constant>
			<Value>0.01</Value>
		</Constant>
		</Schedule>
			</Alpha>
		</Q-Learning>
		</Critic>
			</Critic>
		</Actor-Critic>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion>
