<Window x:Class="AppXML.Views.ProcessesWindowView"
            
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:z="http://schemas.microsoft.com/expression/2010/interactivity"
             mc:Ignorable="d" Width="655.224" Height="409.776"
             ResizeMode="CanMinimize"
             WindowState="Maximized"
        >

    <ScrollViewer VerticalScrollBarVisibility="Auto">
        <StackPanel>
            <ContentControl x:Name="Manager" />
            <Button x:Name="CloseWindow" IsEnabled="{Binding isOver}" Content="Close" Width="50"/>
        </StackPanel>
    </ScrollViewer>
</Window>
