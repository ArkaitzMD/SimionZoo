<?xml version="1.0"?>
<DEFINITIONS>
  <CLASS Name="CCACLALearner">
    <BRANCH Name="E-Traces" Class="CETraces"/>
    <BRANCH Name="Alpha" Class="CNumericValue-Factory"/>
    <BRANCH Name="Policy" Class="CDeterministicPolicy-Factory"/>
  </CLASS>

  <CLASS Name="CIncrementalNaturalActorCritic">
    <BRANCH Name="V-ETraces" Class="CETraces"/>
    <BRANCH Name="U-ETraces" Class="CETraces"/>
    <BRANCH Name="Alpha-v" Class="CNumericValue-Factory"/>
    <BRANCH Name="Alpha-r" Class="CNumericValue-Factory"/>
    <BRANCH Name="Gamma" Class="CNumericValue-Factory"/>
    <BRANCH Name="Gamma" Class="CNumericValue-Factory"/>
    <BRANCH Name="Alpha-u" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CActorCritic">
    <BRANCH Name="Actor" Class="CActor"/>
    <BRANCH Name="Critic" Class="CCritic-Factory"/>
  </CLASS>

  <CLASS Name="CRegularPolicyGradientLearner">
    <BRANCH Name="E-Traces" Class="CETraces"/>
    <BRANCH Name="Alpha" Class="CNumericValue-Factory"/>
    <BRANCH Name="Policy" Class="CDeterministicPolicy-Factory"/>
  </CLASS>

  <CLASS Name="CActor">
    <STRING-VALUE Name="Load" Default=""/>
    <STRING-VALUE Name="Save" Default=""/>
    <MULTI-VALUED Name="Outputs" Class="CPolicyLearner-Factory"/>
  </CLASS>

  <CLASS Name="CController-Factory">
    <CHOICE Name="Controller">
      <CHOICE-ELEMENT Name="Vidal" Class="CWindTurbineVidalController"/>
      <CHOICE-ELEMENT Name="Boukhezzar" Class="CWindTurbineBoukhezzarController"/>
      <CHOICE-ELEMENT Name="PID" Class="CPIDController"/>
      <CHOICE-ELEMENT Name="LQR" Class="CLQRController"/>
    </CHOICE>
  </CLASS>

  <CLASS Name="CLQRGain">
    <XML-NODE-REF Name="Variable" XMLFile="WORLD-DEFINITION" HangingFrom="State"/>
    <DOUBLE-VALUE Name="Gain" Default="0.0"/>
  </CLASS>

  <CLASS Name="CLQRController">
    <XML-NODE-REF Name="Output-Action" XMLFile="WORLD-DEFINITION" HangingFrom="Action"/>
    <MULTI-VALUED Name="LQR-Gain" Class="CLQRGain"/>
  </CLASS>

  <CLASS Name="CPIDController">
    <XML-NODE-REF Name="Input-Variable" XMLFile="WORLD-DEFINITION" HangingFrom="State"/>
    <XML-NODE-REF Name="Output-Action" XMLFile="WORLD-DEFINITION" HangingFrom="Action"/>
    <BRANCH Name="KP" Class="CNumericValue-Factory"/>
    <BRANCH Name="KI" Class="CNumericValue-Factory"/>
    <BRANCH Name="KD" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CWindTurbineVidalController">
    <BRANCH Name="A" Class="CNumericValue-Factory"/>
    <BRANCH Name="K_alpha" Class="CNumericValue-Factory"/>
    <BRANCH Name="KP" Class="CNumericValue-Factory"/>
    <BRANCH Name="KI" Class="CNumericValue-Factory"/>
    <BRANCH Name="P_s" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CWindTurbineBoukhezzarController">
    <DOUBLE-VALUE Name="J_t" Default="0.0"/>
    <DOUBLE-VALUE Name="K_t" Default="0.0"/>
    <BRANCH Name="C_0" Class="CNumericValue-Factory"/>
    <BRANCH Name="KP" Class="CNumericValue-Factory"/>
    <BRANCH Name="KI" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CWindTurbineJonkmanController">
    <DOUBLE-VALUE Name="CornerFreq" Default="0.0"/>
    <DOUBLE-VALUE Name="VSRtGnSp" Default="0.0"/>
    <DOUBLE-VALUE Name="VS_SlPc" Default="0.0"/>
    <DOUBLE-VALUE Name="VS_Rgn2K" Default="0.0"/>
    <DOUBLE-VALUE Name="VS_Rgn2Sp" Default="0.0"/>
    <DOUBLE-VALUE Name="VS_CtInSp" Default="0.0"/>
    <DOUBLE-VALUE Name="VS_RtPwr" Default="0.0"/>
    <DOUBLE-VALUE Name="VS_Rgn3MP" Default="0.0"/>
    <DOUBLE-VALUE Name="PC_RefSpd" Default="0.0"/>
    <BRANCH Name="PC_KK" Class="CNumericValue-Factory"/>
    <BRANCH Name="PC_KP" Class="CNumericValue-Factory"/>
    <BRANCH Name="PC_KI" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CTDLambdaCritic">
    <BRANCH Name="E-Traces" Class="CETraces"/>
    <BRANCH Name="Alpha" Class="CNumericValue-Factory"/>
    <BRANCH Name="Gamma" Class="CNumericValue-Factory"/>
    <STRING-VALUE Name="Load" Default=""/>
    <STRING-VALUE Name="Save" Default=""/>
    <BRANCH Name="V-Function" Class="CLinearStateVFA"/>
  </CLASS>

  <CLASS Name="CTDCLambdaCritic">
    <BRANCH Name="E-Traces" Class="CETraces"/>
    <BRANCH Name="Alpha" Class="CNumericValue-Factory"/>
    <BRANCH Name="Beta" Class="CNumericValue-Factory"/>
    <BRANCH Name="Gamma" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CTrueOnlineTDLambdaCritic">
    <BRANCH Name="E-Traces" Class="CETraces"/>
    <BRANCH Name="Alpha" Class="CNumericValue-Factory"/>
    <BRANCH Name="Gamma" Class="CNumericValue-Factory"/>
    <STRING-VALUE Name="Load" Default=""/>
    <STRING-VALUE Name="Save" Default=""/>
    <BRANCH Name="V-Function" Class="CLinearStateVFA"/>
  </CLASS>

  <CLASS Name="CCritic">
    <STRING-VALUE Name="Load" Default=""/>
    <STRING-VALUE Name="Save" Default=""/>
    <BRANCH Name="V-Function" Class="CLinearStateVFA"/>
  </CLASS>

  <CLASS Name="CCritic-Factory">
    <CHOICE Name="Critic">
      <CHOICE-ELEMENT Name="TD-Lambda" Class="CTDLambdaCritic"/>
      <CHOICE-ELEMENT Name="True-Online-TD-Lambda" Class="CTrueOnlineTDLambdaCritic"/>
      <CHOICE-ELEMENT Name="TDC-Lambda" Class="CTDCLambdaCritic"/>
    </CHOICE>
  </CLASS>

  <CLASS Name="CETraces">
    <DOUBLE-VALUE Name="Threshold" Default="0.001"/>
    <DOUBLE-VALUE Name="Lambda" Default="0.9"/>
    <ENUM-VALUE Name="Replace" Class="Boolean" Default="True"/>
  </CLASS>

  <CLASS Name="CExperiment">
    <INTEGER-VALUE Name="Random-Seed" Default="1"/>
    <INTEGER-VALUE Name="Num-Episodes" Default="1"/>
    <INTEGER-VALUE Name="Eval-Freq" Default="0"/>
    <DOUBLE-VALUE Name="Progress-Update-Freq" Default="0.5"/>
    <DOUBLE-VALUE Name="Episode-Length" Default="1.0"/>
  </CLASS>

  <CLASS Name="CGaussianRBFStateGridFeatureMap">
    <MULTI-VALUED Name="RBF-Grid-Dimension" Class="CStateVariableGrid"/>
  </CLASS>

  <CLASS Name="CGaussianRBFActionGridFeatureMap">
    <MULTI-VALUED Name="RBF-Grid-Dimension" Class="CActionVariableGrid"/>
  </CLASS>

  <CLASS Name="CFeatureMap-Factory">
    <CHOICE Name="Feature-Map">
      <CHOICE-ELEMENT Name="RBF-State-Grid" Class="CGaussianRBFStateGridFeatureMap"/>
      <CHOICE-ELEMENT Name="RBF-Action-Grid" Class="CGaussianRBFActionGridFeatureMap"/>
    </CHOICE>
  </CLASS>

  <ENUMERATION Name="Boolean">
    <VALUE>False</VALUE>
    <VALUE>True</VALUE>
  </ENUMERATION>
  <ENUMERATION Name="Distribution">
    <VALUE>linear</VALUE>
    <VALUE>quadratic</VALUE>
    <VALUE>cubic</VALUE>
  </ENUMERATION>
  <ENUMERATION Name="Interpolation">
    <VALUE>linear</VALUE>
    <VALUE>quadratic</VALUE>
    <VALUE>cubic</VALUE>
  </ENUMERATION>
  <ENUMERATION Name="TimeReference">
    <VALUE>episode</VALUE>
    <VALUE>experiment</VALUE>
    <VALUE>experimentTime</VALUE>
  </ENUMERATION>
  <CLASS Name="CLogger">
    <DOUBLE-VALUE Name="Log-Freq" Default="0.25"/>
    <STRING-VALUE Name="Prefix" Default=""/>
    <DIR-PATH-VALUE Name="Output-Directory" Default="../logs"/>
    <ENUM-VALUE Name="Log-eval-episodes" Class="Boolean" Default="False"/>
    <ENUM-VALUE Name="Log-training-episodes" Class="Boolean" Default="False"/>
    <ENUM-VALUE Name="Log-eval-experiment" Class="Boolean" Default="True"/>
    <ENUM-VALUE Name="Log-training-experiment" Class="Boolean" Default="False"/>
  </CLASS>

  <CLASS Name="CNoise-Factory">
    <CHOICE Name="Noise">
      <CHOICE-ELEMENT Name="GaussianNoise" Class="CGaussianNoise"/>
    </CHOICE>
  </CLASS>

  <CLASS Name="CGaussianNoise">
    <BRANCH Name="Sigma" Class="CNumericValue-Factory"/>
    <BRANCH Name="Alpha" Class="CNumericValue-Factory"/>
    <BRANCH Name="Scale" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CConstantValue">
    <DOUBLE-VALUE Name="Value" Default="0.0"/>
  </CLASS>

  <CLASS Name="CInterpolatedValue">
    <DOUBLE-VALUE Name="Start-Offset" Default="0.0"/>
    <DOUBLE-VALUE Name="Pre-Offset-Value" Default="0.0"/>
    <DOUBLE-VALUE Name="Initial-Value" Default="0.0"/>
    <DOUBLE-VALUE Name="End-Value" Default="1.0"/>
    <DOUBLE-VALUE Name="Evaluation-Value" Default="0.0"/>
    <ENUM-VALUE Name="Interpolation" Class="Interpolation" Default="linear"/>
    <ENUM-VALUE Name="Time-reference" Class="TimeReference" Default="experiment"/>
  </CLASS>

  <CLASS Name="CBhatnagarSchedule">
    <DOUBLE-VALUE Name="Alpha-0" Default="1.0"/>
    <DOUBLE-VALUE Name="Alpha-c" Default="1.0"/>
    <DOUBLE-VALUE Name="Time-Exponent" Default="1.0"/>
    <DOUBLE-VALUE Name="Evaluation-Value" Default="0.0"/>
    <ENUM-VALUE Name="Time-reference" Class="TimeReference" Default="linear"/>
  </CLASS>

  <CLASS Name="CNumericValue-Factory">
    <CHOICE Name="Schedule">
      <CHOICE-ELEMENT Name="Constant" Class="CConstantValue"/>
      <CHOICE-ELEMENT Name="Linear-Schedule" Class="CInterpolatedValue"/>
      <CHOICE-ELEMENT Name="Bhatnagar-Schedule" Class="CBhatnagarSchedule"/>
    </CHOICE>
  </CLASS>

  <CLASS Name="CPolicyLearner">
    <BRANCH Name="Policy" Class="CDeterministicPolicy-Factory"/>
  </CLASS>

  <CLASS Name="CPolicyLearner-Factory">
    <CHOICE Name="Policy-Learner">
      <CHOICE-ELEMENT Name="CACLA" Class="CCACLALearner"/>
      <CHOICE-ELEMENT Name="Regular-Gradient" Class="CRegularPolicyGradientLearner"/>
    </CHOICE>
  </CLASS>

  <CLASS Name="CRewardFunctionComponent">
    <XML-NODE-REF Name="Variable" XMLFile="WORLD-DEFINITION" HangingFrom="State"/>
    <BRANCH Name="Tolerance" Class="CNumericValue-Factory"/>
    <BRANCH Name="Weight" Class="CNumericValue-Factory"/>
  </CLASS>

  <CLASS Name="CRewardFunction">
    <DOUBLE-VALUE Name="Min" Default="-100.0"/>
    <DOUBLE-VALUE Name="Max" Default="1.0"/>
    <MULTI-VALUED Name="Reward-Component" Class="CRewardFunctionComponent"/>
  </CLASS>

  <CLASS Name="CSimGod">
    <MULTI-VALUED Name="Simion" Class="CSimion-Factory"/>
  </CLASS>

  <CLASS Name="CSimion-Factory">
    <CHOICE Name="Simion">
      <CHOICE-ELEMENT Name="Controller" Class="CController-Factory"/>
      <CHOICE-ELEMENT Name="Actor-Critic" Class="CActorCritic"/>
    </CHOICE>
  </CLASS>

  <CLASS Name="CStateVariableGrid">
    <XML-NODE-REF Name="Variable" XMLFile="WORLD-DEFINITION" HangingFrom="State"/>
    <INTEGER-VALUE Name="Num-Features" Default="3"/>
    <DOUBLE-VALUE Name="Min" Default="0.0"/>
    <DOUBLE-VALUE Name="Max" Default="1.0"/>
    <STRING-VALUE Name="Distribution" Default="linear"/>
  </CLASS>

  <CLASS Name="CActionVariableGrid">
    <XML-NODE-REF Name="Variable" XMLFile="WORLD-DEFINITION" HangingFrom="Action"/>
    <INTEGER-VALUE Name="Num-Features" Default="3"/>
    <DOUBLE-VALUE Name="Min" Default="0.0"/>
    <DOUBLE-VALUE Name="Max" Default="1.0"/>
    <STRING-VALUE Name="Distribution" Default="linear"/>
  </CLASS>

  <CLASS Name="CDeterministicPolicy-Factory">
    <CHOICE Name="Policy">
      <CHOICE-ELEMENT Name="Deterministic-Policy-Gaussian-Noise" Class="CDeterministicPolicyGaussianNoise"/>
      <CHOICE-ELEMENT Name="Stochastic-Policy-Gaussian-Noise" Class="CStochasticPolicyGaussianNoise"/>
    </CHOICE>
  </CLASS>

  <CLASS Name="CDeterministicPolicy">
    <XML-NODE-REF Name="Output-Action" XMLFile="WORLD-DEFINITION" HangingFrom="Action"/>
    <BRANCH Name="Linear-State-VFA" Class="CLinearStateVFA"/>
  </CLASS>

  <CLASS Name="CDeterministicPolicyGaussianNoise">
    <BRANCH Name="Exploration-Noise" Class="CNoise-Factory"/>
    <XML-NODE-REF Name="Output-Action" XMLFile="WORLD-DEFINITION" HangingFrom="Action"/>
    <BRANCH Name="Linear-State-VFA" Class="CLinearStateVFA"/>
  </CLASS>

  <CLASS Name="CStochasticPolicyGaussianNoise">
    <BRANCH Name="Sigma-VFA" Class="CLinearStateVFA"/>
    <XML-NODE-REF Name="Output-Action" XMLFile="WORLD-DEFINITION" HangingFrom="Action"/>
    <BRANCH Name="Linear-State-VFA" Class="CLinearStateVFA"/>
  </CLASS>

  <CLASS Name="CLinearStateVFA">
    <DOUBLE-VALUE Name="Init-Value" Default="0.0"/>
    <BRANCH Name="State-Feature-Map" Class="CFeatureMap-Factory"/>
  </CLASS>

  <CLASS Name="CLinearStateActionVFA">
    <DOUBLE-VALUE Name="Init-Value" Default="0.0"/>
    <BRANCH Name="State-Feature-Map" Class="CFeatureMap-Factory"/>
    <BRANCH Name="Action-Feature-Map" Class="CFeatureMap-Factory"/>
  </CLASS>

  <CLASS Name="CMagneticLevitation">
    <FILE-PATH-VALUE Name="Evaluation-Set-Point" Default="../config/world/magnetic-levitation/setpoint.txt"/>
  </CLASS>

  <CLASS Name="CPitchControl">
    <FILE-PATH-VALUE Name="Set-Point-File" Default="../config/world/pitch-control/setpoint.txt"/>
  </CLASS>

  <CLASS Name="CUnderwaterVehicle">
    <FILE-PATH-VALUE Name="Set-Point-File" Default="../config/world/underwater-vehicle/setpoint.txt"/>
  </CLASS>

  <CLASS Name="CWindTurbine">
    <FILE-PATH-VALUE Name="Evaluation-Wind-Data" Default="../config/world/wind-turbine/TurbSim-10.25.hh"/>
    <FILE-PATH-VALUE Name="Power-Set-Point" Default="../config/world/wind-turbine/power-setpoint.txt"/>
    <MULTI-VALUED Name="Training-Wind-Data" Class="FILE-PATH-VALUE"/>
  </CLASS>

  <CLASS Name="CWorld">
    <INTEGER-VALUE Name="Num-Integration-Steps" Default="4"/>
    <DOUBLE-VALUE Name="Delta-T" Default="0.01"/>
    <BRANCH Name="Dynamic-Model" Class="CDynamicModel-Factory"/>
    <BRANCH Name="Reward" Class="CRewardFunction"/>
  </CLASS>

  <CLASS Name="CDynamicModel-Factory">
    <CHOICE Name="Model" LoadXML= "WORLD-DEFINITION">
      <CHOICE-ELEMENT Name="Wind-turbine" Class="CWindTurbine" XML="../config/world/wind-turbine.xml"/>
      <CHOICE-ELEMENT Name="Underwater-vehicle" Class="CUnderwaterVehicle" XML="../config/world/underwater-vehicle.xml"/>
      <CHOICE-ELEMENT Name="Pitch-control" Class="CPitchControl" XML="../config/world/pitch-control.xml"/>
      <CHOICE-ELEMENT Name="Magnetic-leviation" Class="CMagneticLevitation" XML="../config/world/magnetic-levitation.xml"/>
    </CHOICE>
  </CLASS>


</DEFINITIONS>