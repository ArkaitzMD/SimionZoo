  <xs:complexType name="state-action-grid-dimension">
    <xs:sequence>
      <xs:element name="state-action-variable" type="xs:string"/>
      <xs:element name="dimension-num-features" type="xs:integer"/>
      <xs:element name="feature-distribution">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="linear"/>
            <xs:enumeration value="quadratic"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="state-variable-gain">
    <xs:sequence>
      <xs:element name="state-variable" type="xs:string"/>
      <xs:element name="gain" type="xs:decimal"/>
    </xs:sequence>
  </xs:complexType>

</xs:schema>