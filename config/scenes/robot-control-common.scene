  <Objects>
    <Box Name="Floor" TexCoordMultFactor="3">
      <Material>
        <Simple-TL-Material>
          <Texture>
            <Path>concrete1.png</Path>
          </Texture>
        </Simple-TL-Material>
      </Material>
      <Transform>
        <Scale>
          <X>40</X>
          <Y>0.1</Y>
          <Z>40</Z>
        </Scale>
