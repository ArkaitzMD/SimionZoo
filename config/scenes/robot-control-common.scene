<Scene>
  <Camera>
    <Simple-Camera Name="Main-camera">
      <Transform>
        <Translation>
          <X>0.0</X>
          <Y>20.0</Y>
          <Z>20.0</Z>
        </Translation>
        <Rotation>
          <Yaw>0.0</Yaw>
          <Pitch>-1.0</Pitch>
          <Roll>0.0</Roll>
        </Rotation>
      </Transform>
    </Simple-Camera>
  </Camera>
  <Light Name="Main-light">
    <Directional-Light>
      <Direction>
        <X>-0.577</X>
        <Y>0.577</Y>
        <Z>0.577</Z>
      </Direction>
      <Ambient>
        <R>0.2</R>
        <G>0.2</G>
        <B>0.2</B>
      </Ambient>
      <Diffuse>
        <R>1.0</R>
        <G>1.0</G>
