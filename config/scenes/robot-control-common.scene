          <Y>-0.1</Y>
        </Translation>
      </Transform>
    </Box>
    <Box Name="Target" DrawTop="false">
      <Material>
        <Translucent-Material>
          <Texture>
            <Path>translucent-green.tga</Path>
          </Texture>
        </Translucent-Material>
      </Material>
      <Transform>
        <Scale>
          <Y>2</Y>
        </Scale>
        <Translation>
          <X Bind="target-x"/>
          <Z Bind="target-y"/>
        </Translation>
      </Transform>
    </Box>
